# Makefile generated via ChatGPT

CC=gcc
CFLAGS=-Wall

# Default target
all: p1a p1b

# Compile p1a.c to generate p1a executable
p1a: p1a.c
	$(CC) $(CFLAGS) -o p1a p1a.c

# Compile p1b.c to generate p1b executable
p1b: p1b.c
	$(CC) $(CFLAGS) -o p1b p1b.c

# Clean up compiled files
clean:
	rm -f p1a p1b
